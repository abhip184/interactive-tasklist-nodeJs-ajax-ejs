<html>

<head>
    <title>ToDo List</title>
    <script src="https://code.jquery.com/jquery-1.12.3.min.js"></script>
    <link href="/assets/styles.css" rel="stylesheet" ty>
    <script src="/assets/todo-list.js"></script>
</head>

<body>
    <h1>My Todo List</h1>
    <div id="todo-table">
        <form>
            <input type="text" name="item" id="input-bar" placeholder="Add New Item...." required>
            <button type="submit">Add Item</button>
        </form>
        <ul id="todo-ul">
            <% for(var i=0; i < todos.length; i++){ %>
            <li><%= todos[i].item %></li>
            <%}%>
            </ul>
        </div>
    </body>
</html>